<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="SampleVoWorkQueue" class="com.mobon.billing.sample.service.WorkQueueTaskData">
		<constructor-arg index="0" value="SampleVoWorkQueue" />
		<constructor-arg index="1" value="10" />
	</bean>

	<bean id="RetryWorkQueue" class="com.mobon.billing.sample.service.WorkQueue">
		<constructor-arg index="0" value="RetryWorkQueue" />
		<constructor-arg index="1" value="1" />
	</bean>

</beans>
